	<queryString>
		<![CDATA[select  inv.SERIAL, inv.REFERENCE, inv.DELIVERY_DATE, inv.TXNDATE, inv.TXNTIME, inv.CODE, inv.INFO, inv.INFO1, inv.INFO2,inv.RATE_TYPE,
        inv.TOTAL_TAX_EXCLUDED_AMOUNT_VALUE as VergisizAraToplam,
        inv.TOTAL_DISCOUNT_VALUE as SatirIndirimi,
        inv.TOTAL_DOCUMENT_DISCOUNT_VALUE as BelgeIndirimi,
	inv.TOTAL_DISCOUNT_ADDITION_VALUE as VergiDahilIndirim,
        inv.TOTAL_BEFORE_TAX_VALUE as OTVMatrah,
        inv.TAX_EXCLUDED_TOTAL_VALUE as KDVMatrah,
        inv.TOTAL_TAX_VALUE as VergiToplami,
        inv.TOTAL_FEE_VALUE as Harc,
        inv.TOTAL_EXPENSE_VALUE as Masraf,
        inv.TOTAL_EXPENSE_ADDITION_VALUE as VergiDahilMasraf,
	inv.GRAND_TOTAL_VALUE as VergiliSonToplam,
        inv.DELIVERY_FULLNAME as CariAdi,
        inv.DELIVERY_PERSON as CariMi,
        inv.DELIVERY_COMPANY as CariUnvani,
        inv.DELIVERY_SSN as TcKimlikNo,
        inv.DELIVERY_ADDRESS_STREET as CariAdresSokak,
        inv.DELIVERY_ADDRESS_PROVINCE as CariAdresIlce,
        inv.DELIVERY_ADDRESS_CITY as CariAdresSehir,
        inv.DELIVERY_ADDRESS_COUNTRY as CariAdresUlke,
        inv.DELIVERY_ADDRESS_ZIP as CariAdresPostaKodu,
        inv.SHIPPING_DATE as SevkTarihi,
        inv.TAX_NUMBER as VergiNo,
        inv.TAX_OFFICE as VergiDairesi,
        (select f.CODE from WORK_BUNCH f where f.ID = inv.WORKBUNCH_ID) as IsTakip,
        inv.DELIVERER as Veren,
        inv.RECEPIENT as Onaylayan,
        inv.DOC_CURRENCY as BelgeDovizi,

        idet.PRODUCT_TYPE as SatirTuru,
        idet.INFO as SatirAciklamasi,
        idet.LINE_CODE as SatirKodu,
        concat("[",linePr.CODE,"] ",linePr.NAME) as SatirStokAdi,
        concat(idet.QUANTITY, " ",idet.UNIT) as SatirMiktari,
        idet.TAX_EXCLUDED_PRICE_VALUE as SatirBirimFiyat,
        idet.TAX_EXCLUDED_AMOUNT_VALUE as SatirMiktarXBirim,
        idet.BEFORE_TAX_VALUE as SatirOTVMatrahi,
        idet.TAX_EXCLUDED_TOTAL_VALUE as SatirVergiMatrahi,
        idet.TAX_TOTAL_AMOUNT_VALUE as SatirVergiToplami,
        idet.TOTAL_AMOUNT_VALUE as SatirToplamiHarcHaric,
        idet.GRAND_TOTAL_VALUE as SatirNetTutar

from
TEKIR_INVOICE inv
inner join TEKIR_INVOICE_DETAIL idet on idet.OWNER_ID = inv.ID
inner join WAREHOUSE wh on wh.ID = inv.WAREHOUSE_ID
left join ACCOUNT acc on acc.ID = inv.ACCOUNT_ID
left join PAYMENT_TABLE ipt on ipt.ID = inv.PAYMENT_TABLE_ID
left join PRODUCT linePr on linePr.ID = idet.PRODUCT_ID

where
    inv.ISACTIVE = 1
and inv.TRADE_ACTION = 1
and (idet.PRODUCT_TYPE = 1 or idet.PRODUCT_TYPE = 2)
and inv.id = $P{pInvoiceId}]]>

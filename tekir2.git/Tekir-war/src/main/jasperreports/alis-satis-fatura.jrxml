	<queryString>
		<![CDATA[select inv.txndate, inv.action, inv.reference, inv.serial, cn.name as cname, inv.info, inv.code as invcode,
invsi.prdname as sname, invsi.quantity, invsi.unit, invsi.pricevalue, invsi.PRICECCY, inv.TOTALVALUE, inv.TOTALCCY, 
wh.name as wname, invsi.LTAXVALUE, invsi.LTAXCCY, inv.isactive, invsi.linecode, invsi.info as lineinfo,
invsi.TOTALVALUE as detailtotal, invsi.totalccy as detailccy
from INVOICE inv

inner join ( select  CCY, CCYVAL, INFO, LINECODE, LOCALCCY, LOCALVALUE, LTAXCCY, LTAXVALUE, UNIT, QUANTITY, TAXCCY, TAXVALUE, TAXRATE, TOTALCCY, TOTALVALUE, PRICECCY, PRICEVALUE, INVOICE_ID as InvNo, SERVICE_ID as  prdid, service.name as prdname  from INVOICESERVICEITEM inner join service on Service.id = SERVICE_ID
union all
select  CCY, CCYVAL, INFO, LINECODE, LOCALCCY, LOCALVALUE, LTAXCCY, LTAXVALUE, UNIT, QUANTITY, TAXCCY, TAXVALUE, TAXRATE, TOTALCCY, TOTALVALUE, PRICECCY, PRICEVALUE, shipmentnote.invoice_id  as InvNo, PRODUCT_ID as  prdid, product.name as prdname   from SHIPMENTITEM inner join product on product.id = PRODUCT_ID inner join ShipmentNote on  ShipmentNote.id = SHIPMENTITEM.owner_id  )  as invsi on invsi.invno = inv.id
inner join CONTACT cn on cn.ID = inv.CONTACT_ID 
left outer join WAREHOUSE wh on wh.ID = inv.WAREHOUSE_ID
where (  (inv.serial like '%'|| $P{pSeri} ||'%')  )
and (  (inv.reference like '%'|| $P{pRef} ||'%')  )
and (  (inv.code like '%'|| $P{pCode} ||'%')  )
and ( inv.TXNDATE  >= $P{pBDate} and inv.TXNDATE  <= $P{pEDate} )
and ( ( $P{pAct} = 99 ) or ( inv.action = $P{pAct} ) )
group by inv.txndate, inv.action, inv.reference, inv.serial, cn.name, inv.info, inv.code,
invsi.prdname, invsi.quantity, invsi.unit, invsi.pricevalue, invsi.PRICECCY, inv.TOTALVALUE, inv.TOTALCCY, 
wh.name, invsi.LTAXVALUE, invsi.LTAXCCY, inv.isactive, invsi.linecode, invsi.info,invsi.TOTALVALUE,invsi.totalccy
order by inv.TXNDATE, inv.serial, cn.NAME]]>

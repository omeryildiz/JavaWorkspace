	<queryString>
		<![CDATA[select bordro.serial, bordro.reference, cari.fullname as cari_adi, bordro.txndate, bordro.code,bordro.info,
senet.reference_no, senet.payment_place, senet.maturity_date, senet.ENTRY_DATE as alis_tarihi, senet.INFO as aciklama,
senet.ccyval, senet.PROMISSORYNOTE_OWNER as senet_sahibi
from PROM_FROM_CONT_PAY_DET detay
inner join PROM_FROM_CONT_PAYROLL bordro on bordro.id = detay.owner_id
inner join PROMISSORYNOTE senet on senet.id = detay.promissory_id

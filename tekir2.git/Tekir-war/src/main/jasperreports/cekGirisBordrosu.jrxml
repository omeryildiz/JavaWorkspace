	<queryString>
		<![CDATA[select bordro.SERIAL, bordro.REFERENCE, cari.FULLNAME as CARI_ADI, bordro.TXNDATE, bordro.CODE,bordro.INFO,
cek.REFERENCE_NO, cek.PAYMENT_PLACE, cek.MATURITY_DATE,
cek.CCYVAL, cek.CHEQUE_OWNER, cek.BANK_NAME as CEK_BANKA_ADI, cek.INFO as CEK_ACIKLAMA
from CHEQUE_FROM_CONT_PAYROLL_DET detay
inner join CHEQUE_FROM_CONTACT_PAYROLL bordro on bordro.ID = detay.OWNER_ID
inner join CHEQUE cek on cek.ID = detay.CHEQUE_ID
inner join CONTACT cari on cari.ID = bordro.CONTACT_ID
where bordro.ID = $P{pBordroId}]]>

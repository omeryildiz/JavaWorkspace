	<queryString>
		<![CDATA[select
journalEntry.ID, journalEntry.ACCOUNTING_UNIT, journalEntry.BUDGET_YEAR, journalEntry.Entry_DATE, journalEntry.ENTRY_NO, journalEntry.PAYMENT_ORDER_NO,journalEntry.BUDGETAMT, journalEntry.DISCOUNTAMT, journalEntry.STOPPAGEAMT, journalEntry.PAYMENTAMT, journalEntry.FUND_TARNS_NO, journalEntry.INFO,
costCenter.CODE as COST_CENTER_CODE , costCenter.NAME as COST_CENTER_NAME ,
contact.FULLNAME as contactName, contact.PERSON as contactPerson, contact.COMPANY as contactCompany, contact.SSN as contactSsn, contact.TAX_NUMBER as contactTaxNumber ,contact.TAX_OFFICE as contactTaxOffice,
organizer.FULLNAME as organizerName, organizer.PERSON as organizerPerson, organizer.COMPANY as organizerCompany, organizer.SSN as organizerSsn,
officer.FULLNAME as officerName,
chief.FULLNAME as chiefName,
accountant.FULLNAME as accountantName,
authOfficer.FULLNAME as authOfficerName,
authChief.FULLNAME as authChiefName,
bankAccount.ACCOUNT_NO as accountNo,
bankBranch.NAME as branchName

from EPDK_JOURNAL_ENTRY journalEntry
left outer join COST_CENTER costCenter on journalEntry.COST_CENTER_ID=costCenter.ID
left outer join COST_CENTER costCenterParent on costCenter.PARENT_ID=costCenterParent.ID
left outer join CONTACT contact on journalEntry.CONTACT_ID=contact.ID
left outer join CONTACT organizer on journalEntry.ORGANIZER_ID=organizer.ID
left outer join CONTACT officer on journalEntry.CONT_OFFICER_ID = officer.ID
left outer join CONTACT chief on journalEntry.CONT_CHIEF_ID = chief.ID
left outer join CONTACT accountant on journalEntry.CONT_ACCOUNTANT_ID = accountant.ID
left outer join CONTACT authOfficer on journalEntry.AUTH_OFFICER_ID = authOfficer.ID
left outer join CONTACT authChief on journalEntry.AUTH_CHIEF_ID = authChief.ID
left outer join CONTACT_BANK_ACCOUNT contactBankAccount on contactBankAccount.OWNER_ID = contact.ID
left outer join BANK_ACCOUNTS bankAccount on bankAccount.ID = contactBankAccount.BANK_ACCOUNT_ID
left outer join BANK_BRACNHES bankBranch on bankBranch.ID = bankAccount.BANK_BRANCH_ID

where journalEntry.ID = $P{pId}]]>

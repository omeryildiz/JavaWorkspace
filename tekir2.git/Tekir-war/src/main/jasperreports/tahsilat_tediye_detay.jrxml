	<queryString>
		<![CDATA[select py.serial, py.reference, py.txndate, cn.NAME as cari, acc.name as hesap, py.code, py.info, py.action,
pyi.localvalue, pyi.localccy, pyi.linecode, pyi.info as lineinfo
from PAYMENT py
inner join PAYMENTITEM pyi on pyi.owner_id = py.id
inner join ACCOUNT acc on acc.id = py.account_id
inner join ACCOUNTTXN acct on acct.serial = py.serial
inner join CONTACT cn on cn.ID =  py.contact_id
where ( (py.serial like '%'|| $P{pSeri} ||'%')  )
and (  (py.reference like '%'|| $P{pRef} ||'%')  )
and (  (py.code like '%'|| $P{pCode} ||'%')  )
and ( py.TXNDATE  >= $P{pBDate} and py.TXNDATE  <= $P{pEDate} )
and ( ( $P{pAct} = 99 ) or ( py.action = $P{pAct} ) )
group by py.serial, cn.NAME, py.txndate, py.reference, py.code, py.info, py.action,
acc.name, pyi.localvalue, pyi.localccy, pyi.linecode, pyi.info
order by py.TXNDATE, py.SERIAL, cn.NAME]]>

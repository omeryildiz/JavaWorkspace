	<queryString>
		<![CDATA[select cn.code, cn.FULLNAME, cn.COMPANY, cn.person as contact_type, ft.TXN_DATE, ft.INFO, ft.CCY, (ft.LCYVAL/ft.CCYVAL) as kur, ft.CCYVAL,
ft.LCYVAL, ft.reference, ft.DOCUMENT_TYPE, ft.finance_action,
wb.CODE as Iskodu,wb.NAME as Isadi,

(select sum(ft1.ccyval * case when ft1.finance_action = 1 then -1 else 1 end )
	from FINANCE_TXN ft1 where ft1.ISACTIVE=true
	and ft1.CONTACT_ID = $P{pContact}
	and (ft1.txn_date < $P{pBDate})
	and ( ($P{pWorkBunchId} is null) or (ft1.WORK_BUNCH_ID = $P{pWorkBunchId}) )
	and ( ($P{pCode} is null) or (ft1.code like concat($P{pCode} ,'%' )) )
	and ft1.ccy = $P{pCurrency}
	group by ft1.contact_id) as oncekiBakiye

from FINANCE_TXN ft
inner join CONTACT cn on cn.ID=ft.CONTACT_ID
left outer join WORK_BUNCH wb on wb.ID = ft.WORK_BUNCH_ID
where ft.ISACTIVE=true
and ft.contact_id = $P{pContact}
and ( ($P{pWorkBunchId} is null) or (ft.WORK_BUNCH_ID = $P{pWorkBunchId}) )
and ( ($P{pCode} is null) or (ft.code like concat($P{pCode} ,'%' )) )
and ( ft.TXN_DATE  >= $P{pBDate} and ft.TXN_DATE  <= $P{pEDate} )
and ft.ccy = $P{pCurrency}
order by ft.TXN_DATE, ft.SERIAL, ft.ccy]]>

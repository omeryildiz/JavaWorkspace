	<queryString>
		<![CDATA[select pr.name as SNAME, ptxn.trade_action, sum(ptxn.quantity) as TOPLAMSTOK, ptxn.UNIT, ptxn.FINANCE_ACTION
from PRODUCT_TXN ptxn
inner join WAREHOUSE wh on wh.ID = ptxn.WAREHOUSE_ID
inner join PRODUCT pr on pr.ID = ptxn.product_ID
where (pr.code is null) or ( lower(pr.code)  like concat( '%', lower($P{pScode}) ,'%')  )
and (pr.name is null) or ( lower(pr.name) like concat( '%', lower($P{pSname}) ,'%' )  )
and ( ( $P{pScate} = 0 ) or ( pr.Product_CATEGORY_ID = $P{pScate} ) )
and ( ptxn.TXN_DATE >= $P{pBDate} and ptxn.TXN_DATE <= $P{pEDate} )
and ( ( $P{pProductType} = 0 ) or ( ptxn.product_type = $P{pProductType} ) )
and ( ( $P{pWorkBunchId} is null ) or ( ptxn.WORK_BUNCH_ID = $P{pWorkBunchId} ) )
group by pr.name, ptxn.trade_action, ptxn.UNIT, ptxn.finance_action
order by pr.name, ptxn.UNIT]]>
	</queryString>

/* TAHSILAT : kasa nakit hareket gun sonu */
select 1 as DOCTYPE
, 0 as ACTTYPE
, txn.SERIAL as FISNO
, txn.REFERENCE as BELGENO
, txn.TXN_DATE as TXNDATE
, txn.INFO
, txn.LCYVAL as YERELTUTAR
, txn.CCY as DOVIZ
, txn.CCYVAL as ISLEMTUTAR
, txn.Tradein as KAPORAMI
, '' as KARTNO
, '' as CIHAZKOD
, '' as BANKA
, '' as BANKASUBE
, '' as BANKAHESAP
, txn.TXN_DATE as VADEGUNU
, txn.ADVERSE_CODE as ILGILICARIKOD
, txn.ADVERSE_NAME as ILGILICARIADI
, w.NAME as KASAADI
, org.NAME as ORGANIZASYON
, true as PERSON
, txn.ADVERSE_NAME as SIRKET
from ACCOUNT_TXN txn
inner join ACCOUNT w on w.ID = txn.ACCOUNT_ID
left outer join ORGANIZATION org on org.ID = w.ORGANIZATION_ID
where txn.FINANCE_ACTION = 0 and
 ( ( $P{pOrgLevel} = 0 ) or ( org.ID = $P{pOrgLevel} ) ) and
 ( ( $P{pAccount} = 0 ) or ( txn.ACCOUNT_ID = $P{pAccount} ) ) and
 ( txn.TXN_DATE >= $P{pBDate} and txn.TXN_DATE  <= $P{pEDate} )

order by org.ID, txn.ACCOUNT_ID, txn.CCY, txn.Tradein, txn.TXN_DATE
)
union all
(
/* TAHSILAT : pos hareket gun sonu */
select 2 as DOCTYPE
, 0 as ACTTYPE

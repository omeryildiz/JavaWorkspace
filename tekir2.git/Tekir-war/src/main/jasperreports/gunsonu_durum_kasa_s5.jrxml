inner join BANK_BRACNHES bb on bb.ID = p.BANK_BRANCH_ID
inner join BANK_ACCOUNTS ba on ba.ID = p.BANK_ACCOUNT_ID
inner join PAYMENT atxn on atxn.ID = txn.DOCUMENT_ID and txn.DOCUMENT_TYPE = 8
inner join ACCOUNT w on w.ID = atxn.ACCOUNT_ID
inner join CONTACT con on con.ID = atxn.CONTACT_ID
left outer join ORGANIZATION org on org.ID = w.ORGANIZATION_ID
where txn.FINANCE_ACTION = 0 and
 ( ( $P{pOrgLevel} = 0 ) or ( org.ID = $P{pOrgLevel} ) ) and
 ( ( $P{pAccount} = 0 ) or ( atxn.ACCOUNT_ID = $P{pAccount} ) ) and
 ( txn.TXN_DATE >= $P{pBDate} and txn.TXN_DATE  <= $P{pEDate} )


order by org.ID, atxn.ACCOUNT_ID, txn.POS_ID, txn.CCY, txn.Tradein, txn.TXN_DATE

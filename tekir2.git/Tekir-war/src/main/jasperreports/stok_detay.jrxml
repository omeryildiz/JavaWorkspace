	<queryString>
		<![CDATA[select prd.code, prd.name, prdc.code as category, prd.info, prd.barcode, prd.unit, taxb.name as buy, taxs.name as sell, prd.isactive
from product prd
left outer join PRODUCTCATEGORY prdc on prdc.id = prd.CATEGORY_ID
inner join TAX taxb on taxb.id = prd.BUYTAX_ID
inner join TAX taxs on taxs.id = prd.SELLTAX_ID
where ( prd.code like '%'|| $P{pCode} ||'%' )
and ( prd.name like '%'|| $P{pName} ||'%' )
and ( prd.barcode like '%'|| $P{pBarcode} ||'%' )
and ( ( $P{pCate} = 0 ) or ( prdc.id = $P{pCate} ) )
order by prd.name]]>
	</queryString>

	<queryString>
		<![CDATA[select ser.code, ser.name, serc.code as category, ser.info, tax.name as tax, ser.unit, ser.isactive
from service ser
inner join SERVICECATEGORY serc on serc.id = ser.CATEGORY_ID
inner join TAX tax on tax.id = ser.TAX_ID
where ( ser.code like '%'|| $P{pCode} ||'%' )
and ( ser.name like '%'|| $P{pName} ||'%' )
and ( ( $P{pCate} = 0 ) or ( serc.id = $P{pCate} ) )
order by ser.name]]>
	</queryString>

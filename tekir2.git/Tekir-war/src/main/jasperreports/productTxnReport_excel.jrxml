	<queryString>
		<![CDATA[select pt.DOCUMENT_TYPE, pt.SERIAL, pt.REFERENCE, pt.TXN_DATE, wh.name as DEPO,
pr.name as STOK, pr.CODE, pt.INFO, pt.CODE as BELGEKOD,
pt.TRADE_ACTION, pt.ADVERSE_NAME,
pt.ADVERSE_CODE , pg.CODE as MKOD, pg.Name as MARKA, wb.CODE as WBCODE, wb.NAME as WBNAME
,pt.DOCUMENTID as belgeid, pt.PRODUCT_ID as urunid
,pt.UNIT, pt.QUANTITY, pt.CCYVAL, pt.CCY, pt.LCYVAL
,(select avg(tsnd.TAX_EXCLUDED_PRICE_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as birimFiyatVergisizYerel
,(select avg(tsnd.TAX_EXCLUDED_AMOUNT_VALUE) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarFiyatVergisizYerel
,(select avg(tsnd.DISCOUNT_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarIndirimYerel
,(select avg(tsnd.BEFORE_TAX_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarVergidenOnceIndMasYerel
,(select min(tsnd.TAX1_TAX_INCLUDED) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as kdvDahilmi
,(select avg(tsnd.TAX1_RATE) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as kdvOran
,(select avg(tsnd.TAX1_VALUE) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as kdvTutar
,(select avg(tsnd.TOTAL_AMOUNT_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarFiyatVergiliYerel
,(select avg(tsnd.EXPENSE_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarMasrafYerel
,(select avg(tsnd.FEE_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as tutarHarcYerel
,(select avg(tsnd.GRAND_TOTAL_LCYVAL) from TEKIR_SHIPMENT_NOTE tsn
  inner join TEKIR_SHIPMENT_NOTE_DETAIL tsnd on tsnd.OWNER_ID = tsn.ID
  where tsn.ID = pt.DOCUMENTID and tsnd.PRODUCT_ID = pt.PRODUCT_ID ) as satirBelgeEkliToplamYerel

from PRODUCT_TXN pt
inner join PRODUCT pr on pr.id = pt.PRODUCT_ID
inner join WAREHOUSE wh on wh.id = pt.WAREHOUSE_ID
left outer join PRODUCT_GROUP pg on pg.id = pr.GROUP_ID
left outer join WORK_BUNCH wb on wb.ID = pt.WORK_BUNCH_ID
where ( ( $P{pPCode} is null) or (lower(pr.code) like concat('%', lower($P{pPCode}) ,'%') ) )
and ( ( $P{pCode} is null) or (lower(pt.code) like concat('%', lower($P{pCode}) ,'%') ) )
and ( ( pt.TXN_DATE  >= $P{pBDate} and pt.TXN_DATE  <= $P{pEDate} ) )
and ( ( $P{pDocu} = 0 ) or ( pt.DOCUMENT_TYPE = $P{pDocu} ) )
and ( ( $P{pSeri} is null) or ( lower(pt.serial) like concat( '%', lower($P{pSeri}) ,'%' ) ) )
and ( ( $P{pRef} is null) or ( lower(pt.reference) like concat( '%', lower($P{pRef}) ,'%' ) ) )
and ( ( $P{pName} is null) or ( lower(pr.name) like concat( '%', lower($P{pName}) ,'%' ) ) )
and ( ( $P{pAdvName} is null) or (lower(pt.ADVERSE_NAME) like concat(lower($P{pAdvName}),'%') ))
and ( ( $P{pAdvCode} is null) or (lower(pt.ADVERSE_CODE) like concat(lower($P{pAdvCode}),'%') ))
and ( ( $P{pBarcode} is null) or (pr.BARCODE1 = $P{pBarcode} ))
and ( ( $P{pCate} = 0 ) or ( pr.PRODUCT_CATEGORY_ID = $P{pCate} ) )
and ( ( $P{pWare} = 0 ) or ( pt.WAREHOUSE_ID = $P{pWare} ) )
and ( ( $P{pGroup} = 0 ) or ( pr.GROUP_ID = $P{pGroup} ) )
and ( ( $P{pProductType} = 0 ) or ( pt.product_type = $P{pProductType} ) )
and ( ( $P{pWorkBunchId} is null ) or ( pt.WORK_BUNCH_ID = $P{pWorkBunchId} ) )
order by pt.txn_date, pt.id, pt.warehouse_id, pt.serial, pr.id]]>
	</queryString>

	<queryString>
		<![CDATA[select prd.PRODUCT_TYPE,prd.code, prd.name, prdc.code as category, prd.info
, prd.barcode1, prd.unit, prd.LAST_PURCHASE_PRICE_LCYVAL as AlisFiyat,prd.LAST_SALE_PRICE_LCYVAL as SatisFiyat
from PRODUCT prd
left outer join PRODUCT_CATEGORY prdc on prdc.id = prd.PRODUCT_CATEGORY_ID
where ( ($P{pCode} = "") or ( prd.CODE like concat($P{pCode} ,'%' )))
and ( ($P{pName} = "") or ( prd.NAME like concat('%', $P{pName} ,'%' )))
and ( ($P{pBarcode} = "" ) or ( prd.BARCODE1 like concat('%', $P{pBarcode} )))
and ( (($P{pCate} is null) or ($P{pCate} = 0 )) or ( prd.PRODUCT_CATEGORY_ID = $P{pCate}) )
and ( (($P{pGroupId} is null) or ($P{pGroupId} = 0 )) or (prd.GROUP_ID = $P{pGroupId}) )
and (( $P{pProduct_Type} = 99 ) or ( prd.PRODUCT_TYPE = $P{pProduct_Type} ) )
and ( prd.PRODUCT_TYPE != 3 and prd.PRODUCT_TYPE != 4)
order by prd.CODE]]>
	</queryString>

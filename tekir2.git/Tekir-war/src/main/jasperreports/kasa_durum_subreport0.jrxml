	<queryString>
		<![CDATA[select acct.CCY, sum(CCYVAL * case when acct.finance_action = 1 then -1 else 1 end ) as TOTAL,  coalesce( crr.ASK, 1) as YTLASK,
sum(CCYVAL * case when acct.finance_action = 1 then -1 else 1 end )*coalesce( crr.ASK, 1)  as YTLTOTAL
from ACCOUNT_TXN acct
left outer join CURRENCIES cur on cur.code = acct.ccy
left outer join CURRENCY_PAIR cpr on cpr.HARD_CURRENCY_ID = cur.id
left outer join CURRENCY_RATE crr on crr.CURRENCY_PAIR = cpr.id
where ( (crr.RATE_DATE = CURRENT_DATE) or (crr.RATE_DATE is null) )
and ( acct.code  like concat('%',$P{pCode}) )
and ( acct.TXN_DATE  >= $P{pBDate} and acct.TXN_DATE  <= $P{pEDate} )
and ( ( $P{pAccount} is null ) or ( acct.ACCOUNT_ID = $P{pAccount} ) )
group by acct.ccy, crr.ask
order by acct.ccy]]>
	</queryString>

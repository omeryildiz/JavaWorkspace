	<queryString>
		<![CDATA[select bordro.SERIAL, bordro.REFERENCE, banka.name as BANKA,sube.name as SUBE,hesap.name as HESAP, bordro.TXNDATE, bordro.CODE,bordro.INFO,
cek.REFERENCE_NO, cek.PAYMENT_PLACE, cek.MATURITY_DATE,
cek.CCYVAL, cek.CHEQUE_OWNER, cari.FULLNAME as CARI_ADI,cek.bank_name as CEK_BANKA_ADI
from CHEQUE_TO_BANK_PAYROLL_DETAIL detay
inner join CHEQUE_TO_BANK_PAYROLL bordro on bordro.id = detay.owner_id
inner join CHEQUE cek on cek.id = detay.cheque_id
inner join BANKS banka on banka.id = bordro.bank_id
inner join BANK_BRACNHES sube on sube.id = bordro.bank_branch_id
inner join BANK_ACCOUNTS hesap on hesap.id = bordro.bank_account_id
inner join CONTACT cari on cari.id = cek.contact_id
where bordro.id = $P{pBordroId}]]>

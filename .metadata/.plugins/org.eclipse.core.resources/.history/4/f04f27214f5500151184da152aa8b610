
public class Definitions {

	public static void main(String[] args) {
		int age = 24;
		Integer myAge = 27;
		long longClassicalAge = 100_000_000;
		String myName = "Ömer YILDIZ";
		short shortNumber = 215;
		char charCharacter = 'a';
		double doubleNumber = 23.26;
		float floatNumber = 272.45f;
		byte byteNumber = 10;
		System.out.println(age+" "+myAge+" "+longClassicalAge+" "+ myName + " "+shortNumber+" "+charCharacter );
		System.out.println(byteNumber+doubleNumber+floatNumber);
		
		
		

	}

}

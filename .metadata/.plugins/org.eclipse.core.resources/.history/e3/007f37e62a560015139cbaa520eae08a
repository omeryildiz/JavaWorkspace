package tr.gov.tubitak.demo;
//araba sýnýfý oluþtur 4 farklý eriþime sahip metod ve deðiþkenleri tanýmlayýn
//Bu sýnýfý
public class Car {
	public String name;
	public String year;
	public Tyre tyre;
	public HeadLight headlight;

	public Car(String name, String year) {
		this.name = name;
		this.year = year;
	}

	public class Tyre {
		public String brand;
		public int depth;

		public Tyre(String brand, int depth) {
			this.brand = brand;
			this.depth = depth;

		}

		public Tyre() {

		}

	}

	public class HeadLight {
		public String type;

		public HeadLight(String type) {
			this.type = type;
		}

		public HeadLight() {

		}
	}

}

package tr.gov.tubitak.demo;

public class Run {

	public static void main(String[] args) {
		Student st = new Student();
		st.setName("Ömer YILDIZ");
		st.setAge(27);
		st.doHomework(3);
		System.out.println(st.isAdult());
		if(!st.isAdult())
		{
			st.doHomework();
		}
		StudentPrinter sp = new StudentPrinter();
		sp.print(st);

	}

}

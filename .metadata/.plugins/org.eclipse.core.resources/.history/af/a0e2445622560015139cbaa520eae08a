package tr.gov.tubitak.demo;

public class Car {
	public String name;
	public String year;
	public Tyre tyre;
	public HeadLight headlight;
	
	public Car(String name, String year, Tyre tyre, HeadLight headlight)
	{
		this.name = name;
		this.year = year;
		new Tyre(this,tyre);
		new HeadLight(headlight);
	}

	public class Tyre {
		public String brand;
		public int depth;

		public Tyre(Car car, Tyre tyre) {
			this.brand = tyre.brand;
			this.depth = tyre.depth;

		}

	}

	public class HeadLight {
		public String type;

		public HeadLight(HeadLight headlight) {
			this.type = headlight.type;
		}
	}
	
}
